<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<!-- AUTOSAR Datatypes, Rawan Sleem -->
		<AR-PACKAGE>
			<SHORT-NAME>datatypes</SHORT-NAME>
			<ELEMENTS>

				<!-- Define Physical Dimension -->
				<PHYSICAL-DIMENSION>
					<SHORT-NAME>distance_meter</SHORT-NAME>
					<LENGTH-EXP>1</LENGTH-EXP>
				</PHYSICAL-DIMENSION>

				<!-- Define unit -->
				<UNIT>
					<SHORT-NAME>meter</SHORT-NAME>
					<FACTOR-SI-TO-UNIT>1</FACTOR-SI-TO-UNIT>
					<PHYSICAL-DIMENSION-REF
						DEST="PHYSICAL-DIMENSION">/datatypes/distance_meter</PHYSICAL-DIMENSION-REF>
				</UNIT>

				<!-- Define Computation Method (Scaling) -->
				<COMPU-METHOD>
					<SHORT-NAME>comp_distance</SHORT-NAME>
					<CATEGORY>IDENTICAL</CATEGORY>
					<UNIT-REF DEST="UNIT">/datatypes/meter</UNIT-REF>
				</COMPU-METHOD>

				<!-- Define ADT -->
				<APPLICATION-PRIMITIVE-DATA-TYPE>
					<SHORT-NAME>my_meter</SHORT-NAME>
					<CATEGORY>VALUE</CATEGORY>
					<SW-DATA-DEF-PROPS>
						<SW-DATA-DEF-PROPS-VARIANTS>
							<SW-DATA-DEF-PROPS-CONDITIONAL>
								<COMPU-METHOD-REF DEST="COMPU-METHOD">/datatypes/</COMPU-METHOD-REF>
							</SW-DATA-DEF-PROPS-CONDITIONAL>
						</SW-DATA-DEF-PROPS-VARIANTS>
					</SW-DATA-DEF-PROPS>
				</APPLICATION-PRIMITIVE-DATA-TYPE>

				<!-- Define array ADT -->
				<APPLICATION-ARRAY-DATA-TYPE>
					<SHORT-NAME>data_array</SHORT-NAME>
					<CATEGORY>ARRAY</CATEGORY>
					<ELEMENT>
						<SHORT-NAME>array_element</SHORT-NAME>
						<CATEGORY>VALUE</CATEGORY>
						<TYPE-TREF DEST="APPLICATION-PRIMITIVE-DATA-TYPE">/datatypes/my_meter</TYPE-TREF>
						<ARRAY-SIZE-SEMANTICS>FIXED-SIZE</ARRAY-SIZE-SEMANTICS>
						<MAX-NUMBER-OF-ELEMENTS>5</MAX-NUMBER-OF-ELEMENTS>
					</ELEMENT>
				</APPLICATION-ARRAY-DATA-TYPE>

				<!-- Define record (struct) ADT -->
				<APPLICATION-RECORD-DATA-TYPE>
					<SHORT-NAME>data_record</SHORT-NAME>
					<ELEMENTS>
						<APPLICATION-RECORD-ELEMENT>
							<SHORT-NAME>element1</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-PRIMITIVE-DATA-TYPE">/datatypes/my_meter</TYPE-TREF>
						</APPLICATION-RECORD-ELEMENT>
					</ELEMENTS>
				</APPLICATION-RECORD-DATA-TYPE>

				<APPLICATION-RECORD-DATA-TYPE>
					<SHORT-NAME>data_record</SHORT-NAME>
					<ELEMENTS>
						<APPLICATION-RECORD-ELEMENT>
							<SHORT-NAME>element2</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-PRIMITIVE-DATA-TYPE">/datatypes/my_meter</TYPE-TREF>
						</APPLICATION-RECORD-ELEMENT>
					</ELEMENTS>
				</APPLICATION-RECORD-DATA-TYPE>

				<!-- Define BASE SW datatype -->
				<SW-BASE-TYPE>
					<SHORT-NAME>my_sint8</SHORT-NAME>
					<CATEGORY>FIXED_LENGTH</CATEGORY>
					<BASE-TYPE-SIZE>8</BASE-TYPE-SIZE>
					<BASE-TYPE-ENCODING>2C</BASE-TYPE-ENCODING>
					<MEM-ALIGNMENT>8</MEM-ALIGNMENT>
					<NATIVE-DECLARATION>char</NATIVE-DECLARATION>
				</SW-BASE-TYPE>

				<SW-BASE-TYPE>
					<SHORT-NAME>my_uint8</SHORT-NAME>
					<CATEGORY>FIXED_LENGTH</CATEGORY>
					<BASE-TYPE-SIZE>8</BASE-TYPE-SIZE>
					<BASE-TYPE-ENCODING>NONE</BASE-TYPE-ENCODING>
					<NATIVE-DECLARATION>unsigned char</NATIVE-DECLARATION>
				</SW-BASE-TYPE>

				<!-- Define Data Constraint -->
				<DATA-CONSTR>
					<SHORT-NAME>dc_uint8</SHORT-NAME>
					<DATA-CONSTR-RULES>
						<DATA-CONSTR-RULE>
							<PHYS-CONSTRS>
								<LOWER-LIMIT>8</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</PHYS-CONSTRS>
							<INTERNAL-CONSTRS>
								<LOWER-LIMIT>8</LOWER-LIMIT>
								<UPPER-LIMIT>255</UPPER-LIMIT>
							</INTERNAL-CONSTRS>
						</DATA-CONSTR-RULE>
					</DATA-CONSTR-RULES>
				</DATA-CONSTR>

				<!-- Define IDT -->
				<IMPLEMENTATION-DATA-TYPE>
					<SHORT-NAME>uint8</SHORT-NAME>
					<SW-DATA-DEF-PROPS>
						<SW-DATA-DEF-PROPS-VARIANTS>
							<SW-DATA-DEF-PROPS-CONDITIONAL>
								<BASE-TYPE-REF DEST="SW-BASE-TYPE">/datatypes/my_uint8</BASE-TYPE-REF>
								<DATA-CONSTR-REF DEST="DATA-CONSTR">/datatypes/dc_uint8</DATA-CONSTR-REF>
							</SW-DATA-DEF-PROPS-CONDITIONAL>
						</SW-DATA-DEF-PROPS-VARIANTS>
					</SW-DATA-DEF-PROPS>
				</IMPLEMENTATION-DATA-TYPE>

				<!-- Mapping ADT to its IDT -->
				<DATA-TYPE-MAPPING-SET>
					<SHORT-NAME>adt_idt_mapping</SHORT-NAME>
					<DATA-TYPE-MAPS>
						<DATA-TYPE-MAP>
							<APPLICATION-DATA-TYPE-REF
								DEST="APPLICATION-ARRAY-DATA-TYPE">/datatypes/my_meter</APPLICATION-DATA-TYPE-REF>
							<IMPLEMENTATION-DATA-TYPE-REF
								DEST="IMPLEMENTATION-DATA-TYPE">/datatypes/uint8</IMPLEMENTATION-DATA-TYPE-REF>
						</DATA-TYPE-MAP>
					</DATA-TYPE-MAPS>
				</DATA-TYPE-MAPPING-SET>




















			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>