<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<!-- Composite SWC, Rawan Sleem -->
		<AR-PACKAGE>
			<SHORT-NAME>Composite_SWC</SHORT-NAME>
			<ELEMENTS>
				<!-- Define Interfaces -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>Light_Control</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Data</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>Light_Actuate</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Control_Decision</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- Define SWCs -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightControl_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_LightData</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Light_Control</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>ControlApplication_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_LightData</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Light_Control</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_Decision</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Light_Actuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightActuate_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_Decision</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Light_Actuate</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- Define Composition SWC -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Composite1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_Decision_Comp1</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Composite_SWC/Light_Actuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LightControl_SWC_Instance</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightControl_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>ControlApplication_SWC_Instance</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/ControlApplication_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Light_Control_assembly</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Composite1/LightControl_SWC_Instance</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/LightControl_SWC/PP_LightData</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Composite1/ControlApplication_SWC_Instance</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/LightActuate_SWC/RP_Decision</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>PP_Decision_delegate</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF
										DEST="SW-COMPONENT-PROTOTYPE">/Composite1/ControlApplication_SWC_Instance</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/ControlApplication_SWC/Light_Actuate</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/Composite1/PP_Decision_Comp1</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Top_Level_Composite</SHORT-NAME>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>Composite1_Instance</SHORT-NAME>
							<TYPE-TREF DEST="COMPOSITION-SW-COMPONENT-TYPE">/Composite1</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LightActuate_SWC_Instance</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightActuate_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Light_Actuate_assembly</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Composite1/ControlApplication_SWC_Instance</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/ControlApplication_SWC/PP_Decision</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Top_Level_Composite/LightActuate_SWC_Instance</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/LightActuate_SWC/RP_Decision</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>